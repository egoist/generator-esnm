{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "jest && xo",
    "toc": "markdown-toc -i README.md"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],<% if (cli) { %>
  "dependencies": {
    "cac": "^2.3.0"
  },<% } %>
  "devDependencies": {
    "jest-cli": "^15.1.1",
    "xo": "latest"
  },
  "xo": {
    "semicolon": <%= semicolon %>,
    "space": 2,
    "rules": {
      "quote-props": ["error", "as-needed"],
      "no-unused-vars": [2, { "vars": "all", "args": "none" }],
      "curly": [
        "error",
        "multi-line",
        "consistent"
      ]
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
