{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],<% if (cli) { %>
  "dependencies": {
    "cac": "^2.3.0"
  },<% } %>
  "devDependencies": {
    "jest-cli": "^15.1.1",
    "eslint": "latest",
    "eslint-config-egoist": "latest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": "egoist/esnext"
  }
}
